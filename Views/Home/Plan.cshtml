@{
    ViewData["Title"] = "Home Page";
}
@model wedding.Models.WeddingReg
<h1>Plan a Wedding</h1>
<a href="/dashboard">Dashboard</a>
<a href="/logout">Logout</a>
<form asp-controller="Home" asp-action="planprocess" method="post">
    <label asp-for="Wedder1"></label>
    <input asp-for="Wedder1"/>
    <span asp-validation-for="Wedder1"></span><br>
    
    <label asp-for="Wedder2"></label>
    <input asp-for="Wedder2"/>
    <span asp-validation-for="Wedder2"></span><br>
    
    <label asp-for="Date"></label>
    <input type="date" asp-for="Date"/>
    <span asp-validation-for="Date"></span><br>
    
    <label asp-for="Address"></label>
    <input asp-for="Address"/>
    <span asp-validation-for="Address"></span><br>
    
    <button type="submit">Create</button>
</form>